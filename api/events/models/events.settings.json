{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Events",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "EventDate": {
      "type": "date"
    },
    "content": {
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "published",
        "draft"
      ],
      "default": "draft"
    }
  }
}
